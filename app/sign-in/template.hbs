
<div class="row">
  <div class="col s12 m8 offset-m2">
    <div class="card blue-grey darken-1 sign-in-card">
      <div class="card-content white-text">
        <span class="card-title">Sign In</span>
        {{all-forms/sign-in-form credentials=model}}
      </div>
      <div class="card-action">
        <a class="left-align" {{action "signIn" model}}>Sign In</a>
        <a class="right-align" {{action "reset"}}>Cancel</a>
      </div>
    </div>
  </div>
</div>
